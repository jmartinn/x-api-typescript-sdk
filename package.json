{
  "name": "x-api-sdk",
  "version": "0.1.0",
  "description": "A TypeScript SDK for the X API",
  "keywords": [
    "typescript",
    "sdk",
    "api",
    "openapi",
    "developer-tools"
  ],
  "homepage": "https://github.com/jmartinn/x-api-typescript-sdk#readme",
  "bugs": {
    "url": "https://github.com/jmartinn/jmartinn-api-typescript-sdk/issues"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmartinn/x-api-typescript-sdk.git"
  },
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build"
  ],
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@9.8.0",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prebuild": "pnpm clean",
    "generate": "ts-node scripts/generate.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "check": "pnpm run format:check && pnpm run lint",
    "prepublishOnly": "pnpm test",
    "test": "jest",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/jest": "^27.4.1",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "openapi-typescript": "^5.4.1",
    "prettier": "^3.4.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
